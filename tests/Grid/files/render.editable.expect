<div id="snippet-grid-grid">

<form action="/" method="get" id="frm-grid-form" class="ajax grido">

<table id="grid" class="table table-striped table-hover" data-grido-options='{"editable":true}'>
    <thead>
        <tr class="head">
            <th class="checker" rowspan="2">
                <input type="checkbox" title="Invert">
            </th>
                <th class="column grid-header-firstname" data-grido-editable-handler="/?action=default&amp;do=grid-columns-firstname-editable&amp;presenter=Test" data-grido-editableControl-handler="/?action=default&amp;do=grid-columns-firstname-editableControl&amp;presenter=Test">
                        <a class="ajax" href="/?grid-sort%5Bfirstname%5D=asc&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Firstname</a>
                        <span></span>
                </th>
                <th class="column grid-header-surname">
                        <a class="ajax" href="/?grid-sort%5Bsurname%5D=asc&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Surname</a>
                        <span></span>
                </th>
                <th class="column grid-header-gender">
                        <a class="ajax" href="/?grid-sort%5Bgender%5D=asc&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Gender</a>
                        <span></span>
                </th>
                <th class="column grid-header-birthday" data-grido-editable-handler="/?action=default&amp;do=grid-columns-birthday-editable&amp;presenter=Test" data-grido-editableControl-handler="/?action=default&amp;do=grid-columns-birthday-editableControl&amp;presenter=Test">
                        <a class="ajax" href="/?grid-sort%5Bbirthday%5D=asc&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Birthday</a>
                        <span></span>
                </th>
            <th class="actions center">
                Actions
            </th>
        </tr>
        <tr class="filter inner">
                    <th class="grid-filter-firstname">
                        <input type="text" name="filters[firstname]" class="text form-control" id="frm-grid-form-filters-firstname">
                    </th>
                    <th class="grid-filter-surname">
                        <input type="text" name="filters[surname]" class="text form-control" id="frm-grid-form-filters-surname">
                    </th>
                    <th class="grid-filter-gender">
                        <input type="text" name="filters[gender]" class="text form-control" id="frm-grid-form-filters-gender">
                    </th>
                    <th class="grid-filter-birthday">
                        <input type="text" name="filters[birthday]" class="text date form-control" autocomplete="off" id="frm-grid-form-filters-birthday">
                    </th>

            <th class="buttons">
                    <input type="submit" name="buttons[search]" class="btn btn-default btn-sm search" id="frm-grid-form-buttons-search" value="Search">
                    <input type="submit" name="buttons[reset]" class="btn btn-default btn-sm reset" id="frm-grid-form-buttons-reset" value="Reset">
            </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td colspan="6">
                <span class="operations"  title="Select some row">
                    <select name="operations[operations]" class="form-control" id="frm-grid-form-operations-operations"><option value="">Selected...</option><option value="print">Print</option></select>
                    <input type="submit" name="buttons[operations]" class="hide" id="frm-grid-form-buttons-operations" value="OK">
                </span>
                <span class="paginator">
                        <span class="btn btn-default btn-xs btn-mini disabled" href="/?grid-page=0&amp;action=default&amp;do=grid-page&amp;presenter=Test"><i class="glyphicon glyphicon-arrow-left fa fa-arrow-left icon-arrow-left"></i> Previous</span>
                            <span class="btn btn-default btn-xs btn-mini disabled">1</span>
                            <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-page=2&amp;action=default&amp;do=grid-page&amp;presenter=Test">2</a>
                            <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-page=3&amp;action=default&amp;do=grid-page&amp;presenter=Test">3</a>
                            <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-page=4&amp;action=default&amp;do=grid-page&amp;presenter=Test">4</a>
                        <a class="prompt" data-grido-prompt="Enter page:" data-grido-link="/?grid-page=0&amp;action=default&amp;do=grid-page&amp;presenter=Test">...</a>
                            <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-page=7&amp;action=default&amp;do=grid-page&amp;presenter=Test">7</a>
                        <a class="prompt" data-grido-prompt="Enter page:" data-grido-link="/?grid-page=0&amp;action=default&amp;do=grid-page&amp;presenter=Test">...</a>
                            <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-page=10&amp;action=default&amp;do=grid-page&amp;presenter=Test">10</a>
                        <a class="prompt" data-grido-prompt="Enter page:" data-grido-link="/?grid-page=0&amp;action=default&amp;do=grid-page&amp;presenter=Test">...</a>
                            <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-page=13&amp;action=default&amp;do=grid-page&amp;presenter=Test">13</a>
                        <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-page=2&amp;action=default&amp;do=grid-page&amp;presenter=Test">Next <i class="glyphicon glyphicon-arrow-right fa fa-arrow-right icon-arrow-right"></i></a>
                </span>
                <span class="count">
                    Items 1 - 4 of 50
                    <select name="count" title="Items per page" class="form-control" id="frm-grid-form-count"><option value="4" selected>4</option><option value="10">10</option><option value="20">20</option><option value="30">30</option><option value="50">50</option><option value="100">100</option></select>
                        <input type="submit" name="buttons[perPage]" id="frm-grid-form-buttons-perPage" value="Items per page" class="hide">
                    <a class="export btn btn-default btn-xs btn-mini" href="/?action=default&amp;do=grid-export-export&amp;presenter=Test" title="Export all items"><i class="glyphicon glyphicon-download fa fa-download icon-download"></i></a>
                </span>
            </td>
        </tr>
    </tfoot>

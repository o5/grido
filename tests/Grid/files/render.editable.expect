<div id="snippet-grid-grid">



<form action="/test/" method="get" id="frm-grid-form" class="ajax grido" data-grido-refresh-handler="/test/?do=grid-refresh">

<table id="grid" data-grido-options='{"editable":true}'>
    <thead>
        <tr class="head">
            <th class="checker" rowspan="2">
                <input type="checkbox" title="Invert">
            </th>
                <th class="column grid-header-firstname" data-grido-editable-handler="/test/?do=grid-columns-firstname-editable" data-grido-editableControl-handler="/test/?do=grid-columns-firstname-editableControl">
                        <a class="ajax" href="/test/?grid-sort%5Bfirstname%5D=asc&amp;do=grid-sort">Firstname</a>
                </th>
                <th class="column grid-header-surname">
                        <a class="ajax" href="/test/?grid-sort%5Bsurname%5D=asc&amp;do=grid-sort">Surname</a>
                </th>
                <th class="column grid-header-gender">
                        <a class="ajax" href="/test/?grid-sort%5Bgender%5D=asc&amp;do=grid-sort">Gender</a>
                </th>
                <th class="column grid-header-birthday" data-grido-editable-handler="/test/?do=grid-columns-birthday-editable" data-grido-editableControl-handler="/test/?do=grid-columns-birthday-editableControl">
                        <a class="ajax" href="/test/?grid-sort%5Bbirthday%5D=asc&amp;do=grid-sort">Birthday</a>
                </th>
                <th class="actions center">Actions</th>
        </tr>
        <tr class="filter inner">

                    <th class="grid-filter-firstname">
                            <input type="text" name="filters[firstname]" class="text" id="frm-grid-form-filters-firstname">
                    </th>

                    <th class="grid-filter-surname">
                            <input type="text" name="filters[surname]" class="text" id="frm-grid-form-filters-surname">
                    </th>

                    <th class="grid-filter-gender">
                            <input type="text" name="filters[gender]" class="text" id="frm-grid-form-filters-gender">
                    </th>

                    <th class="grid-filter-birthday">
                            <input type="text" name="filters[birthday]" class="text date" autocomplete="off" id="frm-grid-form-filters-birthday">
                    </th>

            <th class="actions">
            </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td colspan="6">
                <span class="search">
                    <input type="submit" name="buttons[search]" value="Search">
                </span>
                <span class="operations"  title="Select some row">
                    <select name="operations[operations]" id="frm-grid-form-operations-operations"><option value="">Selected...</option><option value="print">Print</option></select>
                    <input type="submit" name="buttons[operations]" class="hide" value="OK">
                </span>
                <span class="paginator">

                        <span class="disabled " href="/test/?grid-page=0&amp;do=grid-page"><i class=""></i> Previous</span>
                            <span class="disabled ">1</span>
                            <a class="ajax " href="/test/?grid-page=2&amp;do=grid-page">2</a>
                            <a class="ajax " href="/test/?grid-page=3&amp;do=grid-page">3</a>
                            <a class="ajax " href="/test/?grid-page=4&amp;do=grid-page">4</a>
                        <a class="prompt" data-grido-prompt="Enter page:" data-grido-link="/test/?grid-page=0&amp;do=grid-page">...</a>
                            <a class="ajax " href="/test/?grid-page=7&amp;do=grid-page">7</a>
                        <a class="prompt" data-grido-prompt="Enter page:" data-grido-link="/test/?grid-page=0&amp;do=grid-page">...</a>
                            <a class="ajax " href="/test/?grid-page=10&amp;do=grid-page">10</a>
                        <a class="prompt" data-grido-prompt="Enter page:" data-grido-link="/test/?grid-page=0&amp;do=grid-page">...</a>
                            <a class="ajax " href="/test/?grid-page=13&amp;do=grid-page">13</a>
                        <a class="ajax " href="/test/?grid-page=2&amp;do=grid-page">Next <i class=""></i></a>
                </span>
                <span class="right">
                    <span class="count">
                        <label>Items 1 - 4 of 50</label>
                            <select name="count" title="Items per page" id="frm-grid-form-count"><option value="4" selected>4</option><option value="10">10</option><option value="20">20</option><option value="30">30</option><option value="50">50</option><option value="100">100</option></select>
                        <input type="submit" name="buttons[perPage]" value="Items per page" class="hide">
                    </span>
                    <span class="export">
                        <a href="/test/?do=grid-export-export" title="Export all items">Export</a>
                    </span>
                    <span class="reset">
                        <input type="submit" name="buttons[reset]" value="Reset">
                    </span>
                </span>
            </td>
        </tr>
    </tfoot>
    <tbody>

<div id="snippet-grid-grid">

<form action="/" method="get" id="frm-grid-form" class="ajax grido" data-grido-refresh-handler="/?action=default&amp;do=grid-refresh&amp;presenter=Test">
    <div class="filter outer">
        <div class="items">
            <span class="grid-filter-phone">
                <label for="frm-grid-form-filters-phone">Phone</label>
                <input type="text" name="filters[phone]" class="text form-control" id="frm-grid-form-filters-phone">
            </span>
            <span class="grid-filter-name">
                <label for="frm-grid-form-filters-name">Name</label>
                <input type="text" name="filters[name]" class="text suggest form-control" autocomplete="off" id="frm-grid-form-filters-name" value="a" data-grido-suggest-replacement="-query-" data-grido-suggest-limit="10" data-grido-suggest-handler="/?grid-filters-name-query=-query-&amp;action=default&amp;do=grid-filters-name-suggest&amp;presenter=Test">
            </span>
            <span class="grid-filter-country">
                <label for="frm-grid-form-filters-country">Country</label>
                <input type="text" name="filters[country]" class="text suggest form-control" autocomplete="off" id="frm-grid-form-filters-country" value="au" data-grido-suggest-replacement="-query-" data-grido-suggest-limit="10" data-grido-suggest-handler="/?grid-filters-country-query=-query-&amp;action=default&amp;do=grid-filters-country-suggest&amp;presenter=Test">
            </span>
            <span class="grid-filter-male">

                <label for="frm-grid-form-filters-male"><input type="checkbox" name="filters[male]" class="checkbox form-control" id="frm-grid-form-filters-male" checked>Only male</label>
            </span>
            <span class="grid-filter-tall">

                <label for="frm-grid-form-filters-tall"><input type="checkbox" name="filters[tall]" class="checkbox form-control" id="frm-grid-form-filters-tall">Only tall</label>
            </span>
        </div>
        <div class="buttons">
                    <input type="submit" name="buttons[search]" class="btn btn-default btn-sm search" id="frm-grid-form-buttons-search" value="Search">
                <input type="submit" name="buttons[reset]" class="btn btn-default btn-sm reset" id="frm-grid-form-buttons-reset" value="Reset">
        </div>
    </div>

<table id="grid" class="table table-striped table-hover" data-grido-options='{"editable":true}'>
    <thead>
        <tr class="head">
                <th class="column grid-header-firstname" data-grido-editable-handler="/?action=default&amp;do=grid-columns-firstname-editable&amp;presenter=Test" data-grido-editableControl-handler="/?action=default&amp;do=grid-columns-firstname-editableControl&amp;presenter=Test">
                        <a class="ajax" href="/?grid-sort%5Bfirstname%5D=asc&amp;grid-page=2&amp;grid-filter%5Bname%5D=a&amp;grid-filter%5Bmale%5D=1&amp;grid-filter%5Bcountry%5D=au&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Firstname</a>
                        <span></span>
                </th>
                <th class="column grid-header-surname">
                        Surname
                </th>
                <th class="column grid-header-gender">
                        Gender
                </th>
                <th class="column grid-header-phone">
                        Phone
                </th>
                <th class="column grid-header-country asc">
                        <a class="sort ajax" href="/?grid-sort%5Bcountry%5D=desc&amp;grid-page=2&amp;grid-filter%5Bname%5D=a&amp;grid-filter%5Bmale%5D=1&amp;grid-filter%5Bcountry%5D=au&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Country</a>
                        <span></span>
                </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td colspan="5">
                <span class="paginator">
                        <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-sort%5Bcountry%5D=asc&amp;grid-filter%5Bname%5D=a&amp;grid-filter%5Bmale%5D=1&amp;grid-filter%5Bcountry%5D=au&amp;action=default&amp;do=grid-page&amp;presenter=Test"><i class="glyphicon glyphicon-arrow-left fa fa-arrow-left icon-arrow-left"></i> Previous</a>
                            <a class="btn btn-default btn-xs btn-mini ajax" href="/?grid-sort%5Bcountry%5D=asc&amp;grid-filter%5Bname%5D=a&amp;grid-filter%5Bmale%5D=1&amp;grid-filter%5Bcountry%5D=au&amp;action=default&amp;do=grid-page&amp;presenter=Test">1</a>
                            <span class="btn btn-default btn-xs btn-mini disabled">2</span>
                        <span class="btn btn-default btn-xs btn-mini disabled" href="/?grid-page=3&amp;grid-sort%5Bcountry%5D=asc&amp;grid-filter%5Bname%5D=a&amp;grid-filter%5Bmale%5D=1&amp;grid-filter%5Bcountry%5D=au&amp;action=default&amp;do=grid-page&amp;presenter=Test">Next <i class="glyphicon glyphicon-arrow-right fa fa-arrow-right icon-arrow-right"></i></span>
                </span>
                <span class="count">
                    Items 4 - 6 of 6
                    <select name="count" title="Items per page" class="form-control" id="frm-grid-form-count"><option value="3" selected>3</option><option value="10">10</option><option value="20">20</option><option value="30">30</option><option value="50">50</option><option value="100">100</option></select>
                        <input type="submit" name="buttons[perPage]" id="frm-grid-form-buttons-perPage" value="Items per page" class="hide">
                    <a class="export btn btn-default btn-xs btn-mini" href="/?action=default&amp;do=grid-export-export&amp;presenter=Test" title="Export all items"><i class="glyphicon glyphicon-download fa fa-download icon-download"></i></a>
                </span>
            </td>
        </tr>
    </tfoot>
    <tbody>
            <tr class="grid-row-628">
                    <td class="grid-cell-firstname editable" data-grido-editable-value="Luwam">
                            Luwam
                    </td>
                    <td class="grid-cell-surname">
                            Haylom
                    </td>
                    <td class="grid-cell-gender">
                            male
                    </td>
                    <td class="grid-cell-phone">
                            604-432-8640
                    </td>
                    <td class="grid-cell-country">
                            Australia
                    </td>
            </tr>
            <tr class="grid-row-818">
                    <td class="grid-cell-firstname editable" data-grido-editable-value="Ronald">
                            Ronald
                    </td>
                    <td class="grid-cell-surname">
                            Peacock
                    </td>
                    <td class="grid-cell-gender">
                            male
                    </td>
                    <td class="grid-cell-phone">
                            02752 18 49 11
                    </td>
                    <td class="grid-cell-country">
                            Australia
                    </td>
            </tr>
            <tr class="grid-row-837">
                    <td class="grid-cell-firstname editable" data-grido-editable-value="Dame">
                            Dame
                    </td>
                    <td class="grid-cell-surname">
                            Konƒçan
                    </td>
                    <td class="grid-cell-gender">
                            male

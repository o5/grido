{
    "name": "o5/grido",
    "type": "library",
    "description": "Grido - DataGrid for Nette Framework",
    "keywords": ["datagrid", "datalist", "tablegrid", "grid", "list", "table", "nette"],
    "homepage": "http://o5.github.io/grido-examples/",
    "license": ["BSD-3", "GPL-2.0", "GPL-3.0"],
    "authors": [{
            "name": "Petr BugyÃ­k",
            "homepage": "http://petr.bugyik.cz/"
        }],
    "support": {
        "issues": "https://github.com/o5/grido/issues/",
        "forum": "http://forum.nette.org/cs/13109-grido-datagrid-pro-nette/"
    },
    "autoload": {
        "psr-4": {
            "Grido\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grido\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/tester --colors 1 tests/",
        "syntax": "./vendor/bin/phpcs --standard=standards.xml --colors --encoding=utf-8 -sp src/"
    },
    "require": {
        "php": ">=5.5",
        "nette/utils": "~2.2",
        "nette/forms": "~2.2",
        "latte/latte": "~2.3",
        "nette/application": "~2.2",
        "nette/component-model": "~2.2",
        "symfony/property-access": "~2.6"
    },
    "require-dev": {
        "tracy/tracy": "~2.3",
        "nette/bootstrap": "~2.3",
        "nette/database": "~2.2",
        "nette/tester": "~1.6.1",
        "mockery/mockery": "~0.9.3",
        "dibi/dibi": "~2.3.1",
        "kdyby/doctrine": "~2.3.0",
        "squizlabs/php_codesniffer": "~2.3.0"
    }
}
